<div class='record'>
    <table class='record__body ascii_table'>
        <thead>
            <tr class='record__row'>
            {{#each table.header}}
                <th class='record__cell tx--bold' title='{{ key }}'>{{ abbr }}</th>
            {{/each}}
            </tr>
        </thead>
        <tbody>
        {{#each table.body}}
            <tr class='record__row'>
            {{#each ../table.header}}
                <td class='record__cell' title='{{ this.key }}'>{{ get_value ../this this.abbr }}</td>
            {{/each}}
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>