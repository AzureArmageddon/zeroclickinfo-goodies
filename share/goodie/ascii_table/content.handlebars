<div class="ascii_table--table_container">
    <table class="ascii_table--table">
        <tr class="ascii_table--table_row ascii_table--table_header">
            <th>Dec</th>
            <th>Hex</th>
            <th>Oct</th>
            <th>Html</th>
            <th>Char</th>
        </tr>
        {{#each ascii}}
            <tr class="ascii_table--table_row ascii_table--table_value">
                <td title="Decimal">{{ Dec }}</td>
                <td title="Hexadecimal">{{ Hex }}</td>
                <td title="Octal">{{ Oct }}</td>
                <td title="Html Encoding">{{ Html }}</td>
                <td title="Character Representation">{{ Char }}</td>
            </tr>
        {{/each}}
    </table>
</div>