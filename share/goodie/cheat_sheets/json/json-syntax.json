  "sections": {
    "JSON Object":[
      {
        "key": "var obj = \\{\n\t \"first\": \"John\",\n\t \"last\": \"Doe,\"\n\t \"age\": 39,\n\t \"salary\": 9999\n\\};"
      },
      {
        "key": "obj.age",
        "val": "returns \"39\""
      },
      {
        "key": "obj[\"salary\"]",
        "val": "returns 9999"
      },
      {
        "key": "obj.something",
        "val": "returns undefined"
      }
    ],
    "JSON Value Types": [
      {
        "val": "\"num\" : 12.2",
        "key": "number"
      },
      {
        "val":"\"str\": 'abc'",
        "key": "string"
      },
      {
        "val": "\"bol\": true",
        "key": "boolean"
      },
      {
        "val": "\"arr\": ['a', 0, 1, 'b']",
        "key": "array"
      },
      {
        "val": "\"obj\": {\n\t\"id\": 7,\n\t\"name\": \"duckduckgo\"}",
        "key": "object"
      },
      {
        "val": "\"myNull\" : null",
        "key": "null"
      }
    ],
    "JSON Data":[
      {
        "key":"Key/Value Pair",
        "val":"\"firstname\" : \"John\""
      },
      {
        "key":"Key",
        "val": "A key is always a string enclosed in quotation marks"
      },
      {
        "key":"Value",
        "val": "A value can be a string, number, boolean expression, array, or object"
      }
    ],
    "JSON Array":[
      {
        "key": "var arr = \\[\n\t\\{\"fName\": \"John\", \"lName\": \"Doe\"\\},\n\t\\{\"fName\": \"Anna\", \"lName\": \"Smith\"\\},\n\t\\{\"fName\": \"Peter\", \"lName\": \"Jones\"\\}\n\\];"
      },
      {
        "key": "arr[1]",
        "val": "returns {\"fName\": \"Anna\", \"lName\": \"Smith\"}"
      },
      {
        "key": "arr[0].fName",
        "val": "returns \"John\""
      },
      {
        "key": "arr[5]",
        "val": "returns undefined"
      }
    ]
  }
